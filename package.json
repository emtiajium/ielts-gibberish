{
    "name": "ielts-gibberish",
    "version": "0.1.0",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "create:env": "node -r fs -e \"fs.copyFileSync('.env.sample', '.env', fs.constants.COPYFILE_EXCL)\"",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"migrations/*.ts\" \"src/**/*.ts\" \"test/**/*.ts\"",
        "format:check": "prettier --check \"migrations/*.ts\" \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint:config": "eslint --print-config src/main.ts",
        "lint": "eslint \"{migrations,src,test}/**/*.ts\" --fix",
        "lint:check": "eslint \"{migrations,src,test}/**/*.ts\"",
        "test": "jest --coverage  --config ./test/jest-e2e.json --testTimeout 30000 --verbose --detectOpenHandles --maxWorkers=1 --forceExit --collectCoverageFrom=src/**/*.ts --collectCoverageFrom=!test/**/*  --collectCoverageFrom=!src/main.ts",
        "typeorm": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
    },
    "dependencies": {
        "@nestjs/common": "^7.6.15",
        "@nestjs/config": "^0.6.3",
        "@nestjs/core": "^7.6.15",
        "@nestjs/platform-express": "^7.6.15",
        "@nestjs/swagger": "^4.8.0",
        "@nestjs/typeorm": "^7.1.5",
        "@types/uuid": "^8.3.0",
        "body-parser": "^1.19.0",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cookie-parser": "^1.4.5",
        "dotenv": "^10.0.0",
        "fast-safe-stringify": "^2.0.7",
        "pg": "^8.6.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.7",
        "typeorm": "^0.2.32",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.0",
        "@nestjs/testing": "^7.6.15",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.36",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.25.0",
        "eslint": "^7.22.0",
        "eslint-config-airbnb-typescript": "^12.3.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.23.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-unicorn": "^32.0.1",
        "jest": "^26.6.3",
        "jest-junit": "^12.1.0",
        "prettier": "^2.2.1",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.3"
    }
}
